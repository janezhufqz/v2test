			<deployment table="SapPricingConditions" typecode="10835" />

			<attributes>
			    <attribute qualifier="order" type="java.lang.String">
			        <description>Order number</description>
					<modifiers read="true" write="false" initial="true" search="true"
						       optional="true" unique="true" />
