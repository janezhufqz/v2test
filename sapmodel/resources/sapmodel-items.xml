<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">
	<enumtypes>
	<enumtype code="PricingProceduresSubtotal" generate="true" autocreate="true"
				dynamic="false">
				<description>This enumeration denotes Pricing procedures</description>
				<value code="subtotal1" />
				<value code="subtotal2" />
				<value code="subtotal3" />
				<value code="subtotal4" />
				<value code="subtotal5" />
				<value code="subtotal6" />
			</enumtype>
	
	</enumtypes>

	<itemtypes>
		<itemtype code="Product" autocreate="false" generate="false">
			<attributes>
				<attribute qualifier="blocked" type="java.lang.Boolean">
					<description>Indicator if item is blocked for sale</description>
					<modifiers read="true" write="true" search="true" optional="true" />
					<defaultvalue>Boolean.FALSE</defaultvalue>
					<persistence type="property" />
				</attribute>
				<attribute qualifier="blockedFrom" type="java.util.Date">
					<description>Indicates the date the product is blocked from</description>
					<modifiers read="true" write="true" search="true" optional="true" />

					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>


		<itemtype code="BaseStore" autocreate="false" generate="false">
			<attributes>
				<attribute qualifier="salesOrganization" generate="true" autocreate="true" type="java.lang.String">
					<modifiers read="true" write="true" search="true" optional="true" />
					<persistence type="property" />
				</attribute>


				<attribute qualifier="distributionChannel" generate="true" autocreate="true" type="java.lang.String">
					<modifiers read="true" write="true" search="true" optional="true" />
					<persistence type="property" />
				</attribute>

				<attribute qualifier="division" generate="true" autocreate="true" type="java.lang.String">
					<modifiers read="true" write="true" search="true" optional="true" />
					<persistence type="property" />
				</attribute>
				
				<attribute type="java.lang.Boolean" qualifier="externalCatalogPricingEnabled">
					<persistence type="property" />
					<modifiers optional="false" />
					<defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
					<description>Determines whether the site should use external external catalog pricing</description>
				</attribute>
				<attribute type="java.lang.Boolean" qualifier="externalOrderPricingEnabled">
					<persistence type="property" />
					<modifiers optional="false" />
					<defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
					<description>Determines whether the site should use external external order pricing</description>
				</attribute>
				<attribute type="java.lang.Boolean" qualifier="fallbackToLocalPricingEnabled">
					<persistence type="property" />
					<modifiers optional="false" />
					<defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
					<description>Determines whether the site should allow fallback to local pricing</description>
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="priceSubtotalProcedure">
					<description>Assigned procedure for price.</description>
					<persistence type="property"/>
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="discountsSubtotalProcedure">
					<description>Assigned procedure for discounts.</description>
					<persistence type="property"/>
				</attribute>
				
				<attribute type="PricingProceduresSubtotal" qualifier="taxesSubtotalProcedure">
					<description>Assigned procedure for taxes.</description>
					<persistence type="property"/>
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="deliverySubtotalProcedure">
					<description>Assigned procedure for delivery.</description>
					<persistence type="property"/>
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="paymentCostSubtotalProcedure">
					<description>Assigned procedure for payment.</description>
					<persistence type="property"/>
				</attribute>
			
			</attributes>

		</itemtype>

		<itemtype code="Address" generate="false" autocreate="false">
			<attributes>
				<attribute qualifier="sapCustomerID" type="java.lang.String">
					<description>Unique identifier of SAP customer</description>
					<modifiers initial="true" read="true" write="true" optional="true"/>
					<persistence type="property" />
				</attribute>			
				<attribute qualifier="sapContactID" type="java.lang.String">
					<description>Unique identifier of SAP contact</description>
					<modifiers initial="true" read="true" write="true" optional="true"/>
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>

