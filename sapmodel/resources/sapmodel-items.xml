<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">
	<enumtypes>
		<enumtype code="PricingProceduresSubtotal" generate="true"
			autocreate="true" dynamic="true">
			<description>This enumeration denotes Pricing procedures</description>
			<value code="BRTWR" /> 
			<value code="NETWR" /> 
			<value code="MWSBP" />
			<value code="KZWI1" />
			<value code="KZWI2" />
			<value code="KZWI3" />
			<value code="KZWI4" />
			<value code="KZWI5" />
			<value code="KZWI6" />
		</enumtype>

	</enumtypes>

	<itemtypes>
		<itemtype code="Product" autocreate="false" generate="false">
			<attributes>
				<attribute qualifier="blocked" type="java.lang.Boolean">
					<description>Indicator if item is blocked for sale</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<defaultvalue>Boolean.FALSE</defaultvalue>
					<persistence type="property" />
				</attribute>
				<attribute qualifier="blockedFrom" type="java.util.Date">
					<description>Indicates the date the product is blocked from</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />

					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Address" generate="false" autocreate="false">
			<attributes>
				<attribute qualifier="sapCustomerID" type="java.lang.String">
					<description>Unique identifier of SAP customer</description>
					<modifiers initial="true" read="true" write="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute qualifier="sapContactID" type="java.lang.String">
					<description>Unique identifier of SAP contact</description>
					<modifiers initial="true" read="true" write="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="ReferenceDistributionChannelMapping"
			generate="true" autocreate="true">
			<description>Distribution Channel per Sales Organization - Assign Master Data. Filled from ERP view V_TVKOV_ST</description>
			<attributes>
				<attribute autocreate="true" qualifier="salesOrganization"
					type="java.lang.String">
					<description>Sales Organization</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false"/>
				</attribute>
				<attribute autocreate="true" qualifier="distChannel" type="java.lang.String">
					<description>Distribution Channel</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false"/>
				</attribute>
				<attribute autocreate="true" qualifier="distChannelName"
					type="localized:java.lang.String">
					<description>Distribution Channel Name</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute autocreate="true" qualifier="refDistChannelConditions"
					type="java.lang.String">
					<description>Reference distribution channel for conditions</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false"/>
				</attribute>
				<attribute autocreate="true" qualifier="refDistChannelConditionsName"
					type="localized:java.lang.String">
					<description>Reference distribution channel name for conditions</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute autocreate="true" qualifier="refDistChannelCustMat"
					type="java.lang.String">
					<description>Reference distribution channel for customer and material masters</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false" />
				</attribute>
				<attribute autocreate="true" qualifier="refDistChannelCustMatName"
					type="localized:java.lang.String">
					<description>Reference distribution channel name for customer and material masters</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>

			</attributes>
		</itemtype>

		<itemtype code="ReferenceDivisionMapping" generate="true"
			autocreate="true">
					<description>Divisions per Sales Organization - Assign Master Data. Filled from ERP view V_TVKOS_ST</description>
			<attributes>
				<attribute autocreate="true" qualifier="salesOrganization"
					type="java.lang.String">
					<description>Sales Organization</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false"/>
				</attribute>
				<attribute autocreate="true" qualifier="division" type="java.lang.String">
					<description>Division</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false"/>
				</attribute>
				<attribute autocreate="true" qualifier="divisionName"
					type="localized:java.lang.String">
					<description>Division Name</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute autocreate="true" qualifier="refDivisionConditions"
					type="java.lang.String">
					<description>Reference division for conditions</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false"/>
				</attribute>
				<attribute autocreate="true" qualifier="refDivisionConditionsName"
					type="localized:java.lang.String">
					<description>Reference division name for conditions</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute autocreate="true" qualifier="refDivisionCustomer"
					type="java.lang.String">
					<description>Reference division for customer</description>
					<persistence type="property" />
					<modifiers read="true" write="true" search="true"
						initial="true" optional="false" />
				</attribute>
				<attribute autocreate="true" qualifier="refDivisionCustomerName"
					type="localized:java.lang.String">
					<description>Reference division name for customer</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>

			</attributes>
		</itemtype>


		<itemtype code="SAPConfiguration" autocreate="false" generate="false">
			<attributes>
				<attribute qualifier="referenceCustomer" autocreate="true" type="java.lang.String" generate="true">
					<description>reference customer used in rfc call</description>
				    <persistence type="property" />
				</attribute>
				<attribute qualifier="sapcommon_transactionType" type="java.lang.String" generate="true">
					<description>sapcommon_transactionType</description>
				    <persistence type="property" />
				</attribute>
				<attribute qualifier="salesOrganization" generate="true"
					autocreate="true" type="java.lang.String">
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>

				<attribute qualifier="distributionChannel" generate="true"
					autocreate="true" type="java.lang.String">
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>

				<attribute qualifier="division" generate="true"
					autocreate="true" type="java.lang.String">
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.Boolean" qualifier="externalCatalogPricingEnabled">
					<persistence type="property" />
					<modifiers optional="false" />
					<defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
					<description>Determines whether the site should use external external catalog pricing</description>
				</attribute>
				<attribute type="java.lang.Boolean" qualifier="externalOrderPricingEnabled">
					<persistence type="property" />
					<modifiers optional="false" />
					<defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
					<description>Determines whether the site should use external external order pricing</description>
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="priceSubtotalProcedure">
					<modifiers optional="false" />
					<description>Assigned procedure for price.</description>
					<persistence type="property" />
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="discountsSubtotalProcedure">
					<modifiers optional="false" />
					<description>Assigned procedure for discounts.</description>
					<persistence type="property" />
				</attribute>

				<attribute type="PricingProceduresSubtotal" qualifier="taxesSubtotalProcedure">
					<modifiers optional="false" />
					<description>Assigned procedure for taxes.</description>
					<persistence type="property" />
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="deliverySubtotalProcedure">
					<modifiers optional="false" />
					<description>Assigned procedure for delivery.</description>
					<persistence type="property" />
				</attribute>
				<attribute type="PricingProceduresSubtotal" qualifier="paymentCostSubtotalProcedure">
					<modifiers optional="false" />
					<description>Assigned procedure for payment.</description>
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
		
	</itemtypes>
</items>
